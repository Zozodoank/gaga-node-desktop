{
  "name": "gaganode.desktop",
  "description": "Gaganode Desktop",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "main": "electron/src/main.js",
  "scripts": {
    "download_apphub:mac:x64": "bash prepare.sh -OS darwin -ARCH amd64",
    "download_apphub:win:x64": "bash prepare.sh -OS windows -ARCH amd64",
    "download_apphub:win:arm64": "bash prepare.sh -OS windows -ARCH arm64",
    "vue:dev": "vite --config vue/vite.config.js",
    "vue:build": "vite build --config vue/vite.config.js",
    "vue:preview": "vite preview --config vue/vite.config.js",
    "electron:start": "NODE_ENV=dev electron .",
    "electron:build:mac:x64": "electron-builder -m --x64",
    "electron:build:mac:arm64": "electron-builder -m --arm64",
    "electron:build:win:x64": "electron-builder -w --x64",
    "electron:build:win:arm64": "electron-builder -w --arm64",
    "clean": "shx rm -rf vue/dist && shx rm -rf release"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.1",
    "@codemirror/lang-javascript": "^6.1.3",
    "@codemirror/lang-json": "^6.0.1",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^2.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vueform/multiselect": "^2.6.2",
    "axios": "^1.2.4",
    "axios-mock-adapter": "^1.21.2",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "install": "^0.13.0",
    "moment": "^2.29.4",
    "npm": "^9.4.0",
    "pinia": "^2.0.28",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.45",
    "vue-chartjs": "^5.2.0",
    "vue-codemirror": "^6.1.1",
    "vue-good-table-next": "^0.2.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-sweetalert2": "^5.0.5",
    "vue-tippy": "^6.0.0",
    "vue-toastification": "^2.0.0-rc.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "electron": "^22.3.1",
    "electron-builder": "^23.6.0",
    "mini-svg-data-uri": "^1.4.4",
    "postcss": "^8.4.21",
    "sass": "^1.57.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.2.4",
    "vite": "^4.2.0"
  }
}
